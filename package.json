{
    "name": "hbq-angular-boilerplate",
    "version": "1.0.0",
    "description": "一个完整的angular4开发模板项目",
    "keywords": [
        "angular",
        "angular2",
        "angular4",
        "webpack",
        "typescript"
    ],
    "scripts": {
        "start": "better-npm-run start",
        "tsc": "tsc",
        "lint": "tslint \"src/**/*.ts\"",
        "tree:dir": "treee -l 2 --ignore node_modules/,.git/"
    },
    "betterScripts": {
        "start": {
            "command": "better-npm-run remove && better-npm-run dev"
        },
        "remove": {
            "command": "rimraf dist"
        },
        "dev": {
            "command": "webpack-dev-server --config \"./dev-config/webpack.config.js\" --progress --colors  --hot ",
            "env": {
                "NODE_ENV": "development",
                "IS_PROXY": true
            }
        }
    },
